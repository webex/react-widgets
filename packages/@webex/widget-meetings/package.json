    "es"
  ],
  "dependencies": {
    "@webex/react-component-button-controls": "workspace:*",
    "@webex/react-component-error-display": "workspace:*",
    "@webex/react-component-loading-screen": "workspace:*",
    "@webex/react-component-video": "workspace:*",
    "@webex/react-container-presence-avatar": "workspace:*",
    "@webex/redux-module-avatar": "workspace:*",
    "@webex/redux-module-errors": "workspace:*",
    "@webex/redux-module-meetings": "workspace:*",
    "@webex/redux-module-mercury": "workspace:*",
    "@webex/redux-module-presence": "workspace:*",
    "@webex/redux-module-users": "workspace:*",
    "@webex/webex-widget-base": "workspace:*",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^17.0.2",
    "react-intl": "^5.15.7",
    "react-redux": "^5.0.1",
