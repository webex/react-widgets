{
  "name": "@webex/widget-files",
  "description": "Webex React Files Widget",
  "main": "./cjs/index.js",
  "src": "./src/index.js",
  "module": "./es/index.js",
  "keywords": [],
  "author": "devsupport@webex.com",
  "license": "MIT",
  "repository": "https://github.com/webex/react-widgets",
  "files": [
    "src",
    "dist",
    "cjs",
    "es"
  ],
  "dependencies": {
    "@webex/react-component-activity-menu-header": "workspace:*",
    "@webex/react-component-file-share-display": "workspace:*",
    "@webex/react-container-file-downloader": "workspace:*",
    "@webex/redux-module-conversation": "workspace:*",
    "@webex/redux-module-share": "workspace:*",
    "@webex/webex-widget-base": "workspace:*",
    "classnames": "^2.2.5",
    "core-decorators": "^0.20.0",
    "moment": "^2.17.1",
    "prop-types": "^15.5.10",
    "react": "^17.0.2",
    "react-redux": "^5.0.1",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "reselect": "^3.0.0"
  },
  "version": "0.211.0",
  "scripts": {
    "build": "babel-node ../../../scripts/build/index.js dist widget-files",
    "build:src": "cross-env NODE_ENV=${NODE_ENV:-production} NODE_OPTIONS=--max-old-space-size=2048 yarn run build",
    "serve": "yarn run serve:src package widget-files",
    "serve:src": "cross-env NODE_ENV=${NODE_ENV:-development} babel-node ../../../scripts/start/index.js",
    "publish": "cross-env-shell NODE_ENV='' babel-node ../../../scripts/publish/index.js package widget-files",
    "test:unit": "cd ../../ && yarn run test:unit widget-files"
  },
  "devDependencies": {
    "@babel/node": "^7.8.3",
    "cross-env": "^5.0.0"
  }
