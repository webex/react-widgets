{
  "name": "@ciscospark/react-ciscospark",
  "version": "0.1.164",
  "description": "The Cisco Spark for React library allows developers to easily incorporate Spark functionality into an application.",
  "scripts": {
    "build": "./scripts/build/index.js",
    "build:all": "cross-env NODE_ENV=${NODE_ENV:-production} npm run build all",
    "build:package": "cross-env NODE_ENV=${NODE_ENV:-production} npm run build dist",
    "build:components": "npm run build components",
    "build:packagejson": "npm run build package.json",
    "build:widgets": "npm run build widgets",
    "clean": "npm run clean:dist && npm run clean:transpile",
    "clean:dist": "rimraf \"packages/**/dist/\"",
    "clean:transpile": "rimraf \"packages/**/es/\" && rimraf \"packages/**/cjs/\"",
    "test": "npm run static-analysis && npm run jest",
    "test:clean": "npm run clean && npm run test",
    "test:automation": "wdio wdio.conf.js --suite oneOnOne,space,messageMeet,recents",
    "test:automation:oneOnOne": "wdio wdio.conf.js --suite oneOnOne",
    "test:automation:space": "wdio wdio.conf.js --suite space",
    "test:automation:messageMeet": "wdio wdio.conf.js --suite messageMeet",
    "test:automation:recents": "wdio wdio.conf.js --suite recents",
    "test:tap": "wdio wdio.conf.js --suite tap",
    "jest": "cross-env TZ=utc jest --config=jest.config.json",
    "jest:package": "cross-env NODE_ENV=${NODE_ENV:-production} babel-node --presets=stage-1,es2015 scripts/jest-package.js",
    "static-analysis": "npm run eslint",
    "eslint": "eslint packages",
    "serve": "npm run start",
    "stylelint": "stylelint \"./packages/node_modules/**/*.css\"",
    "start": "cross-env NODE_ENV=${NODE_ENV:-development} ./scripts/start/index.js",
    "start:package": "npm run start package",
    "publish": "cross-env-shell NODE_ENV='' ./scripts/publish/index.js",
    "publish:components": "npm run build:components && npm run build:packagejson && npm run publish components",
    "upgradespark": "./scripts/utils/update.js",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciscospark/react-ciscospark.git"
  },
  "contributors": [
    "Adam Weeks <adweeks@cisco.com>",
    "Bernie Zang <nzang@cisco.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ciscospark/react-ciscospark/issues"
  },
  "homepage": "https://github.com/ciscospark/react-ciscospark#readme",
  "main": "src/index.js",
